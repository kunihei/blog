-# %h1 New plan

-# = render 'form'

-# = link_to '戻る', plans_path


= form_with(model: @plan, local: true) do |form|

  = form.label :title 
  = form.text_field :title

  = form.label :start_time
  = form.datetime_select :start_time

  = form.label :content
  = form.text_field :content

  = form.submit